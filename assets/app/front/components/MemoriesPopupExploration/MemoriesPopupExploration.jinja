{% import "front/utils/macros.jinja" as macros %}

{% macro MemoriesPopupExploration(block={}) %}
    <div class="MemoriesPopup-exploration isAnimated">
        {% for i in range(1, 75) %}
            {% if i < 10 %}
                {% set i = i | string %}
            {% endif %}
            <div class="MemoriesPopup-explorationImage{% if loop.last %} isShow{% endif %}" data-src="{{ static('img/popups/5/sequence/solar-' + i | string + '.jpg') }}"></div>
        {% endfor %}
        <div class="MemoriesPopup-explorationFooter">
            <div class="MemoriesPopup-explorationTitle">Exploration</div>
